  @charset "utf-8";
/* CSS Document */
   	.main{
   		padding:20px 0;
   		font:14px/1.5 "Microsoft Yahei", 
   		"Hiragino Sans GB", Helvetica, 
   		"Helvetica Neue", "微软雅黑", Tahoma, 
   		 Arial, sans-serif;
   	}
   	.wcontainer{
   	 	width:1200px;
   	 	margin:0 auto;
   	 	min-height:660px;
   	}
   	.setting-left{
   	  	list-style:none;
   		width:240px;
   		height:178px;
   		border:1px solid #ddd;
   		background-color:white;
   		margin-left:70px;
   		float:left;
   	}
   	.setting-left a{
   		display: block;
   		text-decoration:none;
   		width:239px;
		height: 59px;
		margin-left:-40px;
		padding-left:40px;
		line-height: 59px;
		font-size: 14px;
		color: #333;
		border-bottom:1px solid #ddd;
   	}
   	.setting-left a:HOVER{
		border-left: 2px solid #be3948;
		color: #be3948;
   	}
   	.setting-left .active{
   		border-left: 1px solid #be3948;
   	}
   	.setting-right{
   		width:800px;
   		min-height:600px;
   		margin-left:20px;
   		padding:50px 100px;
   		border:1px solid #ddd;
   		background-color:white;
   		float:left;
   	}
   	.setting-right .user-info{
   		width:600px;
   		height:450px;
   	}
   	.info-group{
   		height:65px;
   	}
   	.info-group .control-label{
   		float: left;
		width: 120px;
		text-align: right;
		height: 40px;
		line-height: 40px;
		color: #656e73;
		font-size: 14px;
   	}
   	.info-group .userinput{
	   	width: 350px;
		height: 35px;
		line-height: 24px;
		padding: 7px 7px 7px 20px;
		font-size: 14px;
		margin-left:20px;
		float:left;
   	}
   	.info-group select{
   		width: 165px;
		height: 35px;
		line-height: 24px;
		padding: 7px 7px 7px 20px;
		font-size: 14px;
		margin-left:20px;
		float:left;
   	}
   	.radiolabel{
   		width:50px;
   		height:20px;
   		margin-left:20px;
   		margin-top:8px;
   		font-size: 14px;
   		color: #656e73;
   		float:left;
   	}
   	.user-info button{
   		width: 350px;
   		margin-left:140px;
   	}
   	.user-sign{
   		height:150px;
   		margin-left:85px;
   		font-size: 14px;
   		color: #656e73;
   		float:left;
   	}
   	.user-sign label{margin-left:-10px;}
   	.user-sign textarea{
   		width:350px;
   		margin-left:10px;
   		float:right;
   	}